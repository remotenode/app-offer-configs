{
  "name": "app-config-service",
  "version": "1.0.0",
  "description": "Mobile app configuration service based on AppsFlyer and Firebase integration",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:dev": "wrangler deploy --env development",
    "deploy:prod": "wrangler deploy --env production",
    "db:create": "wrangler d1 create app-config-db",
    "db:migrate": "wrangler d1 migrations apply app-config-db",
    "db:studio": "wrangler d1 studio app-config-db",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cloudflare-workers",
    "appsflyer",
    "firebase",
    "mobile-apps",
    "webview",
    "push-notifications"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "wrangler": "^3.19.0"
  },
  "dependencies": {
    "typescript": "^5.3.0"
  }
}